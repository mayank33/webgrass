<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE task SYSTEM "grass-interface.dtd">
<task name="t.rast.to.rast3">
	<description>
		Converts a space time raster dataset into a 3D raster map.
	</description>
	<keywords>
		temporal, conversion, raster, raster3d, voxel, time
	</keywords>
	<parameter name="input" type="string" required="yes" multiple="no">
		<description>
			Name of the input space time raster dataset
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="old" element="strds" prompt="strds" />
	</parameter>
	<parameter name="output" type="string" required="yes" multiple="no">
		<description>
			Name for output 3D raster map
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="new" element="grid3" prompt="raster_3d" />
	</parameter>
	<flag name="overwrite">
		<description>
			Allow output files to overwrite existing files
		</description>
	</flag>
	<flag name="help">
		<description>
			Print usage summary
		</description>
	</flag>
	<flag name="verbose">
		<description>
			Verbose module output
		</description>
	</flag>
	<flag name="quiet">
		<description>
			Quiet module output
		</description>
	</flag>
</task>
