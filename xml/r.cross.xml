<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE task SYSTEM "grass-interface.dtd">
<task name="r.cross">
	<description>
		Creates a cross product of the category values from multiple raster map layers.
	</description>
	<keywords>
		raster, statistics
	</keywords>
	<parameter name="input" type="string" required="yes" multiple="yes">
		<description>
			Names of 2-30 input raster maps
		</description>
		<gisprompt age="old" element="cell" prompt="raster" />
	</parameter>
	<parameter name="output" type="string" required="yes" multiple="no">
		<description>
			Name for output raster map
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="new" element="cell" prompt="raster" />
	</parameter>
	<flag name="z">
		<description>
			Non-zero data only
		</description>
	</flag>
	<flag name="overwrite">
		<description>
			Allow output files to overwrite existing files
		</description>
	</flag>
	<flag name="help">
		<description>
			Print usage summary
		</description>
	</flag>
	<flag name="verbose">
		<description>
			Verbose module output
		</description>
	</flag>
	<flag name="quiet">
		<description>
			Quiet module output
		</description>
	</flag>
</task>
