<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE task SYSTEM "grass-interface.dtd">
<task name="g.gui.psmap">
	<description>
		Tool for creating hardcopy map outputs.
	</description>
	<keywords>
		general, GUI, printing
	</keywords>
	<parameter name="file" type="string" required="no" multiple="no">
		<label>
			File containing mapping instructions to load
		</label>
		<description>
			See ps.map manual for details
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="old" element="file" prompt="file" />
	</parameter>
	<flag name="help">
		<description>
			Print usage summary
		</description>
	</flag>
	<flag name="verbose">
		<description>
			Verbose module output
		</description>
	</flag>
	<flag name="quiet">
		<description>
			Quiet module output
		</description>
	</flag>
</task>
