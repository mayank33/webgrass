<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE task SYSTEM "grass-interface.dtd">
<task name="db.test">
	<description>
		Test database driver, database must exist and set by db.connect.
	</description>
	<keywords>
		database, attribute table
	</keywords>
	<parameter name="test" type="string" required="yes" multiple="no">
		<description>
			Test name
		</description>
		<values>
			<value>
				<name>test1</name>
			</value>
		</values>
	</parameter>
	<flag name="help">
		<description>
			Print usage summary
		</description>
	</flag>
	<flag name="verbose">
		<description>
			Verbose module output
		</description>
	</flag>
	<flag name="quiet">
		<description>
			Quiet module output
		</description>
	</flag>
</task>
