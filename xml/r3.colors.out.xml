<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE task SYSTEM "grass-interface.dtd">
<task name="r3.colors.out">
	<description>
		Exports the color table associated with a 3D raster map.
	</description>
	<keywords>
		raster3d, color table, export
	</keywords>
	<parameter name="map" type="string" required="yes" multiple="no">
		<description>
			Name of 3D raster map
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="old" element="grid3" prompt="raster_3d" />
	</parameter>
	<parameter name="rules" type="string" required="no" multiple="no">
		<label>
			Path to output rules file
		</label>
		<description>
			If not given write to standard output
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="new" element="file" prompt="file" />
	</parameter>
	<flag name="p">
		<description>
			Output values as percentages
		</description>
	</flag>
	<flag name="overwrite">
		<description>
			Allow output files to overwrite existing files
		</description>
	</flag>
	<flag name="help">
		<description>
			Print usage summary
		</description>
	</flag>
	<flag name="verbose">
		<description>
			Verbose module output
		</description>
	</flag>
	<flag name="quiet">
		<description>
			Quiet module output
		</description>
	</flag>
</task>
