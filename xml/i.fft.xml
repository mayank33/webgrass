<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE task SYSTEM "grass-interface.dtd">
<task name="i.fft">
	<description>
		Fast Fourier Transform (FFT) for image processing.
	</description>
	<keywords>
		imagery, transformation, Fast Fourier Transform
	</keywords>
	<parameter name="input" type="string" required="yes" multiple="no">
		<description>
			Name of input raster map
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="old" element="cell" prompt="raster" />
	</parameter>
	<parameter name="real" type="string" required="yes" multiple="no">
		<description>
			Name for output real part arrays stored as raster map
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="new" element="cell" prompt="raster" />
	</parameter>
	<parameter name="imaginary" type="string" required="yes" multiple="no">
		<description>
			Name for output imaginary part arrays stored as raster map
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="new" element="cell" prompt="raster" />
	</parameter>
	<flag name="overwrite">
		<description>
			Allow output files to overwrite existing files
		</description>
	</flag>
	<flag name="help">
		<description>
			Print usage summary
		</description>
	</flag>
	<flag name="verbose">
		<description>
			Verbose module output
		</description>
	</flag>
	<flag name="quiet">
		<description>
			Quiet module output
		</description>
	</flag>
</task>
