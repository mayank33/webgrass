<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE task SYSTEM "grass-interface.dtd">
<task name="r3.timestamp">
	<description>
		Print/add/remove a timestamp for a 3D raster map
	</description>
	<keywords>
		raster3d, timestamp, time, voxel
	</keywords>
	<parameter name="map" type="string" required="yes" multiple="no">
		<description>
			Name of 3D raster map
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="old" element="grid3" prompt="raster_3d" />
	</parameter>
	<parameter name="date" type="string" required="no" multiple="no">
		<description>
			Datetime, datetime1/datetime2, or none
		</description>
		<keydesc>
			<item order="1">timestamp</item>
		</keydesc>
	</parameter>
	<flag name="help">
		<description>
			Print usage summary
		</description>
	</flag>
	<flag name="verbose">
		<description>
			Verbose module output
		</description>
	</flag>
	<flag name="quiet">
		<description>
			Quiet module output
		</description>
	</flag>
</task>
