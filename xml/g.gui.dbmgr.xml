<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE task SYSTEM "grass-interface.dtd">
<task name="g.gui.dbmgr">
	<description>
		Launches graphical attribute table manager.
	</description>
	<keywords>
		general, GUI, attribute table, database
	</keywords>
	<parameter name="map" type="string" required="yes" multiple="no">
		<label>
			Name of vector map
		</label>
		<description>
			Or data source for direct OGR access
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="old" element="vector" prompt="vector" />
	</parameter>
	<flag name="help">
		<description>
			Print usage summary
		</description>
	</flag>
	<flag name="verbose">
		<description>
			Verbose module output
		</description>
	</flag>
	<flag name="quiet">
		<description>
			Quiet module output
		</description>
	</flag>
</task>
