<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE task SYSTEM "grass-interface.dtd">
<task name="g.cairocomp">
	<description>
		Overlays multiple X Pixmaps.
	</description>
	<keywords>
		general, display
	</keywords>
	<parameter name="input" type="string" required="yes" multiple="yes">
		<description>
			Name of input file(s)
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="old" element="file" prompt="file" />
	</parameter>
	<parameter name="output" type="string" required="yes" multiple="no">
		<description>
			Name for output file
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="new" element="file" prompt="file" />
	</parameter>
	<parameter name="visual" type="integer" required="no" multiple="no">
		<description>
			Output Visual XID
		</description>
	</parameter>
	<parameter name="screen" type="integer" required="no" multiple="no">
		<description>
			Output screen
		</description>
	</parameter>
	<parameter name="opacity" type="float" required="no" multiple="yes">
		<description>
			Layer opacities
		</description>
	</parameter>
	<parameter name="width" type="integer" required="yes" multiple="no">
		<description>
			Image width
		</description>
	</parameter>
	<parameter name="height" type="integer" required="yes" multiple="no">
		<description>
			Image height
		</description>
	</parameter>
	<parameter name="bgcolor" type="string" required="no" multiple="no">
		<label>
			Background color (R:G:B:A)
		</label>
		<description>
			Either a standard color name or R:G:B triplet
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="old" element="color" prompt="color" />
	</parameter>
	<flag name="d">
		<description>
			Do not composite; just delete input Pixmaps
		</description>
	</flag>
	<flag name="overwrite">
		<description>
			Allow output files to overwrite existing files
		</description>
	</flag>
	<flag name="help">
		<description>
			Print usage summary
		</description>
	</flag>
	<flag name="verbose">
		<description>
			Verbose module output
		</description>
	</flag>
	<flag name="quiet">
		<description>
			Quiet module output
		</description>
	</flag>
</task>
