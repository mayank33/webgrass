<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE task SYSTEM "grass-interface.dtd">
<task name="i.oif">
	<description>
		Calculates Optimum-Index-Factor table for spectral bands
	</description>
	<keywords>
		imagery, multispectral, statistics
	</keywords>
	<parameter name="input" type="string" required="yes" multiple="yes">
		<description>
			Name of input raster map(s)
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="old" element="cell" prompt="raster" />
	</parameter>
	<parameter name="output" type="string" required="no" multiple="no">
		<description>
			Name for output file (if omitted or "-" output to stdout)
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="new" element="file" prompt="file" />
	</parameter>
	<flag name="g">
		<description>
			Print in shell script style
		</description>
	</flag>
	<flag name="s">
		<description>
			Process bands serially (default: run in parallel)
		</description>
	</flag>
	<flag name="overwrite">
		<description>
			Allow output files to overwrite existing files
		</description>
	</flag>
	<flag name="help">
		<description>
			Print usage summary
		</description>
	</flag>
	<flag name="verbose">
		<description>
			Verbose module output
		</description>
	</flag>
	<flag name="quiet">
		<description>
			Quiet module output
		</description>
	</flag>
</task>
