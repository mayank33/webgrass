<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE task SYSTEM "grass-interface.dtd">
<task name="r.colors.stddev">
	<description>
		Sets color rules based on stddev from a raster map's mean value.
	</description>
	<keywords>
		raster, color table
	</keywords>
	<parameter name="map" type="string" required="yes" multiple="no">
		<description>
			Name of raster map
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="old" element="cell" prompt="raster" />
	</parameter>
	<flag name="b">
		<description>
			Color using standard deviation bands
		</description>
	</flag>
	<flag name="z">
		<description>
			Force center at zero
		</description>
	</flag>
	<flag name="help">
		<description>
			Print usage summary
		</description>
	</flag>
	<flag name="verbose">
		<description>
			Verbose module output
		</description>
	</flag>
	<flag name="quiet">
		<description>
			Quiet module output
		</description>
	</flag>
</task>
