<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE task SYSTEM "grass-interface.dtd">
<task name="v.pack">
	<description>
		Exports a vector map as GRASS GIS specific archive file
	</description>
	<keywords>
		vector, export, copying
	</keywords>
	<parameter name="input" type="string" required="yes" multiple="no">
		<label>
			Name of vector map to pack up
		</label>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="old" element="vector" prompt="vector" />
	</parameter>
	<parameter name="output" type="string" required="no" multiple="no">
		<description>
			Name for output file (default is &lt;input&gt;.pack)
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="new" element="file" prompt="file" />
	</parameter>
	<flag name="c">
		<description>
			Switch the compression off
		</description>
	</flag>
	<flag name="overwrite">
		<description>
			Allow output files to overwrite existing files
		</description>
	</flag>
	<flag name="help">
		<description>
			Print usage summary
		</description>
	</flag>
	<flag name="verbose">
		<description>
			Verbose module output
		</description>
	</flag>
	<flag name="quiet">
		<description>
			Quiet module output
		</description>
	</flag>
</task>
