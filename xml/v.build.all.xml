<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE task SYSTEM "grass-interface.dtd">
<task name="v.build.all">
	<description>
		Rebuilds topology on all vector maps in the current mapset.
	</description>
	<keywords>
		vector, topology
	</keywords>
	<flag name="help">
		<description>
			Print usage summary
		</description>
	</flag>
	<flag name="verbose">
		<description>
			Verbose module output
		</description>
	</flag>
	<flag name="quiet">
		<description>
			Quiet module output
		</description>
	</flag>
</task>
