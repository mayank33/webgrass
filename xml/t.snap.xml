<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE task SYSTEM "grass-interface.dtd">
<task name="t.snap">
	<description>
		Snaps temporally the maps of a space time dataset.
	</description>
	<keywords>
		temporal, snapping, time
	</keywords>
	<parameter name="input" type="string" required="yes" multiple="no">
		<description>
			Name of an existing space time dataset
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<gisprompt age="old" element="stds" prompt="stds" />
	</parameter>
	<parameter name="type" type="string" required="no" multiple="no">
		<description>
			Type of the input space time dataset
		</description>
		<keydesc>
			<item order="1">name</item>
		</keydesc>
		<default>
			strds
		</default>
		<values>
			<value>
				<name>strds</name>
			</value>
			<value>
				<name>stvds</name>
			</value>
			<value>
				<name>str3ds</name>
			</value>
		</values>
		<guisection>
			Required
		</guisection>
		<guidependency>
			input
		</guidependency>
	</parameter>
	<flag name="help">
		<description>
			Print usage summary
		</description>
	</flag>
	<flag name="verbose">
		<description>
			Verbose module output
		</description>
	</flag>
	<flag name="quiet">
		<description>
			Quiet module output
		</description>
	</flag>
</task>
