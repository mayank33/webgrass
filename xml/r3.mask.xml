<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE task SYSTEM "grass-interface.dtd">
<task name="r3.mask">
	<description>
		Establishes the current working 3D raster mask.
	</description>
	<keywords>
		raster3d, mask, voxel
	</keywords>
	<parameter name="map" type="string" required="yes" multiple="no">
		<description>
			3D raster map with reference values
		</description>
		<gisprompt age="old" element="grid3" prompt="3d-raster" />
	</parameter>
	<parameter name="maskvalues" type="string" required="no" multiple="yes">
		<description>
			List of cell values to be masked out
		</description>
		<keydesc>
			<item order="1">val[-val]</item>
		</keydesc>
	</parameter>
	<flag name="help">
		<description>
			Print usage summary
		</description>
	</flag>
	<flag name="verbose">
		<description>
			Verbose module output
		</description>
	</flag>
	<flag name="quiet">
		<description>
			Quiet module output
		</description>
	</flag>
</task>
